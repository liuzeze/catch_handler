<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/item_network_feed_root_layout"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginLeft="8dp"
    android:layout_marginTop="10dp"
    android:layout_marginRight="8dp"
    android:background="#ffffff"
    android:orientation="horizontal">

    <View
        android:id="@+id/item_network_feed_status_view"
        android:layout_width="5dp"
        android:layout_height="match_parent"
        android:layout_gravity="center_vertical"
        android:layout_marginLeft="5dp"
        android:layout_marginTop="10dp"
        android:layout_marginRight="10dp"
        android:layout_marginBottom="10dp"
        android:background="@drawable/green_rect" />

    <RelativeLayout
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_margin="10dp"
        android:layout_weight="1"
        android:orientation="vertical">

        <TextView
            android:id="@+id/item_network_feed_url_textView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="5dp"
            android:text="http://www.xxxx.comwww.xxxx.comwww.xxxx.com"
            android:textColor="#333333"
            android:textSize="17sp" />

        <TextView
            android:id="@+id/item_network_feed_content_type_textView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/item_network_feed_url_textView"
            android:layout_marginTop="10dp"
            android:layout_marginBottom="5dp"
            android:text="ContentType: json"
            android:textColor="#999999"
            android:textSize="14sp" />

        <TextView
            android:id="@+id/item_network_feed_status_textView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/item_network_feed_content_type_textView"
            android:layout_marginBottom="5dp"
            android:gravity="center"
            android:text="StatusCode: 200"
            android:textColor="#8fcc73"
            android:textSize="14sp" />

        <TextView
            android:id="@+id/item_network_feed_method_textView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/item_network_feed_status_textView"
            android:layout_marginBottom="5dp"
            android:text="GET "
            android:textColor="#666666"
            android:textSize="14sp" />

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/item_network_feed_content_type_textView"
            android:layout_alignParentRight="true"
            android:gravity="right"
            android:orientation="vertical">

            <TextView
                android:id="@+id/item_network_feed_cost_time_textView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginBottom="5dp"
                android:text="900 ms"
                android:textColor="#333333"
                android:textSize="15sp" />

            <TextView
                android:id="@+id/item_network_feed_size_textView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginBottom="5dp"
                android:text="900 KB"
                android:textColor="#333333"
                android:textSize="15sp" />


        </LinearLayout>

    </RelativeLayout>

</LinearLayout>